{
  "name": "tomato-timer",
  "version": "1.1.4",
  "scripts": {
    "ng": "ng",
    "config": "ts-node src/assets/scripts/setEnv.ts",
    "start": "npm run config -- --environment=dev && ng serve",
    "build": "npm run config -- --environment=prod && ng build --configuration production && npm run clean-css",
    "clean-css": "cd dist/tomato-timer && purgecss --css ./styles.*.css --content ./index.html ./*.js --output ./ && cd ../..",
    "http-server": "http-server -p 8080 -c-1 dist/tomato-timer",
    "build-locale": "npm run build && npm run http-server",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.11",
    "@angular/common": "13.3.11",
    "@angular/compiler": "13.3.11",
    "@angular/core": "13.3.11",
    "@angular/forms": "13.3.11",
    "@angular/localize": "13.3.11",
    "@angular/platform-browser": "13.3.11",
    "@angular/platform-browser-dynamic": "13.3.11",
    "@angular/router": "13.3.11",
    "@angular/service-worker": "13.3.11",
    "@ng-bootstrap/ng-bootstrap": "12.1.2",
    "@popperjs/core": "^2.10.2",
    "autoprefixer": "10.4.5",
    "aws-rum-web": "1.6.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.3",
    "ngx-google-analytics": "^13.0.1",
    "rxjs": "7.5.5",
    "tslib": "2.4.0",
    "zone.js": "0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.8",
    "@angular/cli": "13.3.8",
    "@angular/compiler-cli": "13.3.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "18.0.0",
    "dotenv": "16.0.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "purgecss": "^4.1.3",
    "ts-node": "10.8.1",
    "tslint": "~6.1.0",
    "typescript": "4.4.3",
    "yargs": "^17.5.1"
  }
}
